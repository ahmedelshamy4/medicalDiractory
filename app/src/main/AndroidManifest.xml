<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mohammed.medicaldirectoryy">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">




        <activity
            android:name="com.mohammed.medicaldirectoryy.SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mohammed.medicaldirectoryy.activity.MainActivity"></activity>
        <activity android:name="com.mohammed.medicaldirectoryy.Register"></activity>





        <activity
            android:name="com.mohammed.medicaldirectoryy.HospitalDetails"
            android:theme="@style/DetailsTheme">
            >
        </activity>
        <activity android:name="com.mohammed.medicaldirectoryy.PharmacyDetails"></activity>


        <activity
            android:name="com.mohammed.medicaldirectoryy.Home"
            android:theme="@style/DetailsTheme">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <uses-feature
            android:glEsVersion="0x00020000"
            android:required="true" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyD5WEpoku0bXGeC5Ddj6EVUdptKSRAaBoU" />
